{
  "extends": ["airbnb", "plugin:import/typescript"],
  "env": {
    "browser": true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
        "jsx": true
    }
  },
  "rules": {
    "import/prefer-default-export": 0,
    "import/extensions": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".tsx"] }]
  }
}
